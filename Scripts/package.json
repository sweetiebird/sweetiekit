    "leftpad": "^0.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "socket.io-client": "^2.2.0",
    "sweetiekit-dom": "^0.0.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ]
  }
}
