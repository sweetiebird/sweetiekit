{
  "name": "node-ios-hello",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build": "npx babel reactkit/components --out-dir dist/components && npx babel reactkit/renderer --out-dir dist/renderer && npx babel reactkit/utils --out-dir dist/utils && npx babel reactkit/index.js --out-file dist/index.js",
    "clean": "rm dist/bundle.js",
    "build-dev": "webpack -d --mode development",
    "build-prod": "webpack -p --mode production",
    "start": "webpack-dev-server --hot --mode development"
  },
  "dependencies": {
    "Faker": "^0.7.2",
    "axios": "^0.18.0",
    "http-server": "^0.11.1",
    "leftpad": "^0.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.2",
    "react-reconciler": "^0.20.4",
    "socket.io-client": "^2.2.0",
    "sweetiekit-dom": "^0.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
